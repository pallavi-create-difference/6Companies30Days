void rotate(int n,int a[][n])
{
    //code here
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            int t=a[i][j];
            a[i][j]=a[j][i];
            a[j][i]=t;
        }
    }
    for(int i=0;i<n/2;i++){
        for(int j=0;j<n;j++){
            int t=a[i][j];
            a[i][j]=a[n-i-1][j];
            a[n-i-1][j]=t;
        }}
            
}
